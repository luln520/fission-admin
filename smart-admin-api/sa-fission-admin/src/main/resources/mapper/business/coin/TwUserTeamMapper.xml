<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="net.lab1024.sa.admin.module.system.TwAdmin.dao.TwUserTeamMapper">
    <select id="listpage" resultType="net.lab1024.sa.admin.module.system.TwAdmin.entity.TwUserTeam">
        select * from tw_user_team where 1=1 and company_id  = #{obj.companyId}
        <if test="obj.userCode != null and obj.userCode !=''">
            and uid = #{obj.userCode}
        </if>
        <if test="obj.employeeId != null and obj.employeeId !=''">
            and path like concat('%',#{obj.employeeId},'%')
        </if>
        order by create_time desc
    </select>
</mapper>
