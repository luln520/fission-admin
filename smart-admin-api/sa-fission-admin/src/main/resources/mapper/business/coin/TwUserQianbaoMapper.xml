<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="net.lab1024.sa.admin.module.system.TwAdmin.dao.TwUserQianbaoDao">

    <select id="listpage" resultType="net.lab1024.sa.admin.module.system.TwAdmin.entity.TwUserQianbao">
        select * from tw_user_qianbao where 1=1  and company_id  = #{obj.companyId}
        <if test="obj.username != null and obj.username !=''">
            and coinname = #{obj.username}
        </if>
        order by id desc
    </select>

</mapper>